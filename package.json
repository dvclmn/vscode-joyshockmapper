{
  "name": "vscode-joyshockmapper",
  "displayName": "JoyShockMapper Language Support",
  "description": "Language support and auto-completion for JoyShockMapper *.txt configuration files.",
  "author": "dvclmn",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
		"joyshock",
		"joyshockmapper",
		"JSM"
	],
  "contributes": {
    "languages": [{
      "id": "joyshockconfig",
      "aliases": ["Joyshock Mapper", "joyshockconfig"],
      "extensions": [".txt"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "joyshockconfig",
      "scopeName": "jmc",
      "path": "./syntaxes/joyshockconfig.tmLanguage.json"
    }]
  }
}
