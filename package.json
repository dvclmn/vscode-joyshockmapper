{
  "name": "vscode-joyshockmapper",
  "displayName": "JoyShockMapper config language",
  "description": "Language support and auto-completion for JoyShockMapper *.txt configuration files.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "jsmconfig",
      "aliases": ["JoyShockMapper config", "jsmconfig"],
      "extensions": [".txt"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "jsmconfig",
      "scopeName": "text.jsmconfig",
      "path": "./syntaxes/jsmconfig.tmLanguage.json"
    }]
  }
}
